<!DOCTYPE html>
<html>
<head>
<script src="http://code.jquery.com/jquery-latest.js">
</script>
<script>
$(document).ready(function () {

$("#but1").click(function () {
$("#div1").fadeIn();
$("#div2").fadeOut("slow");

});

$("#but2").click(function () {
$("#div1").fadeIn();
$("#div2").fadeIn(2000);

});
});
</script>
</head>
<body>
<button id="but1">Click me</button>
<button id="but2">Click me</button>
<br><br>
<div id="div1" style="width:80px;display:none;height:80px;background-color:red;"></div><br>
<div id="div2" style="width:80px;display:none;height:80px;background-color:green;"></div><br>

</body>
